<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="id1"
         point="org.eclipse.e4.workbench.model">
      <fragment
            apply="initial"
            uri="fragment/CommandPanelFragment.e4xmi">
      </fragment>
   </extension>
   <extension
         point="Goko.org.goko.ui.gkPreferencePage">
      <page
            class="org.goko.tools.commandpanel.preferences.CommandPanelPreferencePage"
            id="org.goko.tools.commandpanel"
            name="Command panel">
      </page>
   </extension>
   <extension
         point="Goko.org.goko.ui.gkPreferenceStoreProvider">
      <provider
            class="org.goko.tools.commandpanel.preferences.CommandPanelPreferenceStoreProvider"
            id="org.goko.tools.commandpanel.provider">
      </provider>
   </extension>
   <extension
         point="org.eclipse.equinox.preferences.preferences">
      <initializer
            class="org.goko.tools.commandpanel.preferences.CommandPanelPreferenceInitializer">
      </initializer>
   </extension>

</plugin>
